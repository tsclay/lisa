<% if (typeof errors != 'undefined') { %>
<div
  class="alert alert-warning alert-dismissible fade show"
  role="alert"
  id="error-msg"
>
  <%= errors.msg %>
  <button type="button" class="close" data-dismiss="alert" aria-label="Close">
    <span aria-hidden="true">&times;</span>
  </button>
</div>
<% } %>
